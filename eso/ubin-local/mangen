#!/usr/bin/zsh

if [[ $1 == -i ]]; then
	read -A "items?Enter author, examplecmdline,progdescflag,progdesc,nonflagdesc,progname,website,year"
	set -- "$items"
fi

typeset -g  ___authors___=${5-$(echo ${${(@s.,.)${${(@s.:.)$(cat /etc/passwd | grep osirisgothra)}[5]}}[1]})}
typeset -g  ___example_cmdline___=${4-<no example available>}
typeset -g  ___prog_and_flag_description___=${3-example option}
typeset -g  ___prog_description___=${2-program description}
typeset -g  ___prog_noflags_description___=${6-without flags, does its default thing}
typeset -g  ___progname___=${1-noname program}
typeset -g  ___website___=${7-secret hidden website for elite users}
typeset -g  ___year___=${8-2019}
typeset -g __seealso_items___=${9-$___progname___}

src="
.\\\" DO NOT MODIFY THIS FILE!  It was generated by help2man 1.43.3.
.TH $___progname___ \"1\" \"October 2015\" \"GNU coreutils 8.23\" \"User Commands\"
.SH NAME
$___progname___ \\- $___prog_noflags_description___
.SH SYNOPSIS
.B $___progname___
[\\fIOPTION\\fR]... [\\fIFILE\\fR]...
.SH DESCRIPTION
.\\\" Add any additional description here
.PP
$___prog_description___
.TP
\\fB\\-A\\fR, \\fB\\-\\-show\\-all\\fR
explain in plain or explain in \\fB\\bold_text\\fR
.TP
\\fB\\-\\-help\\fR
display this help and exit
.TP
\\fB\\-\\-version\\fR
output version information and exit
.PP
With no FILE, or when FILE is \\-, gets standard input as the FILE instead.
.SH EXAMPLES
.TP
$___progname___ $___example_cmdline___
$___prog_and_flag_description___
.TP
$___progname___
$___prog_noflags_description___
.SH AUTHOR
Written by $___authors___
.SH \"REPORTING BUGS\"
$___progname___ online help: <$___website___>
.br
Report $___progname___ translation bugs to <http://translationproject.org/team/$___progname___>
.SH COPYRIGHT
Copyright \\(co $___year___ $___authors___
License GPLv3+: GNU GPL version 3 or later <http://gnu.org/licenses/gpl.html>.
.br
This is free software: you are free to change and redistribute it.
There is NO WARRANTY, to the extent permitted by law.
.SH \"SEE ALSO\"
\\fB$___progname___ $___seealso_items___\\fP(1)
.PP
.br
Full documentation at: <$___website___>
.br
or available locally via: info \\(aq$___progname___\\(aq
"
echo $src
